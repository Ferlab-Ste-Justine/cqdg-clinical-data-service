[
    {
        "conditions": {
            "all": [
                {
                    "fact": "study",
                    "path": "$.participants.length",
                    "operator": "greaterThan",
                    "value": "0"
                }
            ]
        },
        "event": {
            "type": "participants-not-found"
        },
        "name": "validate-has-participants"
    },
    {
        "conditions": {
            "all": [
                {
                    "fact": "study",
                    "path": "$.participants[*].dob",
                    "operator": "dateIsNotAfter",
                    "value": {
                        "fact": "study",
                        "path": "$.participants[*].date_of_recruitement"
                    }
                }
            ]
        },
        "event": {
            "type": "participants-born-after-recruitment-date"
        },
        "name": "validate-participants-born-before-recruitment-date"
    }
]
